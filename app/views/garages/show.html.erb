<%- model_class = Garage -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @garage.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dimension) %>:</strong></dt>
  <dd><%= @garage.dimension %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observation) %>:</strong></dt>
  <dd><%= @garage.observation %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @garage.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reference) %>:</strong></dt>
  <dd><%= @garage.reference %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @garage.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @garage.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:length) %>:</strong></dt>
  <dd><%= @garage.length %></dd>
  <dt><strong><%= model_class.human_attribute_name(:accuracy) %>:</strong></dt>
  <dd><%= @garage.accuracy %></dd>
  <dt><strong><%= model_class.human_attribute_name(:profile_id) %>:</strong></dt>
  <dd><%= @garage.profile_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:district_id) %>:</strong></dt>
  <dd><%= @garage.district_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              garages_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_garage_path(@garage), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              garage_path(@garage),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
